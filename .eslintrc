{
   "parser": "babel-eslint",
   "env": {
      "es6": true,
      "browser": true,
      "node":true
   },
   "rules": {
      "semi":"off",
      "linebreak-style":"off",
      "arrow-parens":"off",
      "indent":"off",
      "comma-dangle":"off",
      "no-trailing-spaces":"off",
      "eol-last":"off",
      "require-jsdoc":"off"
   },
   "extends": [
      "eslint:recommended",
      "google"
   ]
}